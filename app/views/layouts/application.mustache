<!doctype html>
<html>
<head>
  <title>{{title}}{{^title}}Welcome to ExploreUK{{/title}}</title>
  {{#css}}
  <link rel="stylesheet" href="{{href}}">
  {{/css}}
  {{#js}}
  <script src="{{href}}?hash={{hash}}"></script>
  {{/js}}
</head>
<body>
  {{> nav}}
  <div id="content">
  <div class='container-fluid'>
    <div class='row'>
      {{{content}}}
      <div class="tab-content">
        <div id="metadata" class="tab-pane fade active in">
          {{{toc}}}
        </div>
        <div id="access" class="tab-pane fade">
          {{{requests}}}
        </div>
      </div>
    </div>
  </div>
  </div>
  {{>footer}}
  {{>viewer}}
</body>
</html>
